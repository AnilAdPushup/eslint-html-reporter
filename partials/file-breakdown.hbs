<h2>File Breakdown</h2>

<table class="report-table">
  <thead>
    <tr>
      <th>File</th>
      <th class="error-col">Errors</th>
      <th class="warning-col">Warnings</th>
    </tr>
  </thead>
  <tbody>
    {{#each this}}
      {{#row this}}
        <td>{{path}}</td>
        <td>{{errors}}</td>
        <td>{{warnings}}</td>
      {{/row}}
    {{/each}}
  </tbody>
</table>
